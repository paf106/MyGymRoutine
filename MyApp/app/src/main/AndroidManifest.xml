<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.MyGymRoutine.myapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.activity.profile.ModifyPasswordActivity"></activity>
        <activity android:name=".view.activity.profile.ProfileDataActivity" />
        <activity android:name=".view.activity.NavigationActivity" />
        <activity android:name=".view.activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.register.RegisterActivity" />
        <activity android:name=".view.activity.login.LogInActivity" />
    </application>

</manifest>